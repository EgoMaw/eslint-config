{
    "name": "@egomaw/eslint-config",
    "version": "1.1.1",
    "description": "My own twist on a basic eslint config that conforms to my preferred write style",
    "main": "index.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./index.js",
            "require": "./index.js"
        },
        "./index": {
            "import": "./index.js",
            "require": "./index.js"
        },
        "./vue": {
            "import": "./vue.js",
            "require": "./vue.js"
        },
        "./react": {
            "import": "./react.js",
            "require": "./react.js"
        },
        "./react-javascript": {
            "import": "./react-js.js",
            "require": "./react-js.js"
        },
        "./typescript": {
            "import": "./typescript.js",
            "require": "./typescript.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/EgoMaw/eslint-config.git"
    },
    "keywords": [
        "eslint",
        "eslint-config"
    ],
    "author": "Kimon \"EgoMaw\" <egomaw@dedicatedmc.io>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/EgoMaw/eslint-config/issues"
    },
    "homepage": "https://github.com/EgoMaw/eslint-config#readme",
    "scripts": {
        "lint": "eslint --config index.js",
        "lint:fix": "eslint --config index.js --fix",
        "visualize": "pnpm dlx @eslint/config-inspector@latest --config .\\index.js",
        "visualize:ts": "pnpm dlx @eslint/config-inspector@latest --config .\\typescript.js",
        "visualize:vue": "pnpm dlx @eslint/config-inspector@latest --config .\\vue.js",
        "visualize:react": "pnpm dlx @eslint/config-inspector@latest --config .\\react.js",
        "visualize:react-js": "pnpm dlx @eslint/config-inspector@latest --config .\\react-js.js"
    },
    "dependencies": {
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-prettier": "^5.2.6",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-vue": "^10.0.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "typescript-eslint": "^8.29.1",
        "vue-eslint-parser": "^10.1.3"
    },
    "peerDependencies": {
        "@eslint/js": ">=9.7.0",
        "eslint": ">=9.7.0"
    }
}
